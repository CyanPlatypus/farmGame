<Window x:Class="IncubatorStrategy.ShopWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ShopWindow" Height="300" Width="300" Loaded="Window_Loaded">
    <StackPanel Name="shopStackPanel">
        <TabControl Name="shopTabControl">
            <TabItem Header="Sections" Name="sectionsTabItem">
            </TabItem>
            <TabItem Header="Items" Name="itemsTabItem">
            </TabItem>
        </TabControl>
        <StackPanel Name="currencyStackPanel" Height="30"/>
    </StackPanel>
</Window>
